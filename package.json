{
  "name": "@brainervirus.dev/personal-portfolio",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
