{
  "name": "@brainervirus.dev/personal-portfolio",
  "type": "module",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "commit": "git-cz",
    "check": "astro check && tsc --noEmit",
    "lint": "eslint --ext .astro,.ts,.js,.tsx .",
    "lint:fix": "eslint --ext .astro,.ts,.js,.tsx . --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "semantic-release": "semantic-release",
    "test": "pnpm exec playwright test",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/check": "0.8.2",
    "@astrojs/mdx": "3.1.3",
    "@astrojs/tailwind": "5.1.0",
    "@astrojs/vercel": "7.7.2",
    "animate.css": "4.1.1",
    "astro": "4.12.2",
    "astro-icon": "1.1.0",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.43.1",
    "@iconify-json/bi": "1.1.23",
    "@iconify-json/cib": "1.1.8",
    "@iconify-json/grommet-icons": "1.1.10",
    "@iconify-json/logos": "1.1.42",
    "@iconify-json/mdi": "1.1.66",
    "@iconify-json/simple-icons": "1.1.100",
    "@iconify-json/teenyicons": "1.1.9",
    "@playwright/test": "1.43.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "20.12.7",
    "@typescript-eslint/parser": "6.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "daisyui": "^4.12.10",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-astro": "0.34.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "prettier-plugin-astro-organize-imports": "0.4.7",
    "prettier-plugin-tailwindcss": "0.5.14",
    "semantic-release": "23.0.8",
    "tailwind-merge": "^2.4.0"
